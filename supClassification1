/***** End of imports. If edited, may not auto-convert in the playground. *****/
// Import country boundaries feature collection.
var dataset = ee.FeatureCollection('USDOS/LSIB_SIMPLE/2017');
// Apply filter where country name equals Ethiopia.
var eth = dataset.filter(ee.Filter.eq('country_na', 'Ethiopia'));
// There should only be one feature representing Ethiopia.
print('Ethiopia feature collection:', eth);
// Convert the Ethiopia boundary feature collection to a line for map display.
var ethBorder =
    ee.Image().byte().paint({featureCollection: eth, color: 1, width: 3});
// Set map options and add the Ethiopia boundary as a layer to the map.
Map.setOptions('SATELLITE');
Map.centerObject(eth, 6);
Map.addLayer(ethBorder, null, 'Ethiopia border');
var L8 = ee.ImageCollection('LANDSAT/LC08/C01/T1_TOA')
 .filterBounds(eth)
 .filterDate('2018-01-01', '2021-12-31')
 .filterMetadata('CLOUD_COVER', 'less_than', 1)
 .mean()
 .clip(eth)
 
 Map.addLayer(L8, {Bands:["B5", "B4", "B3"]},'ROI_Ethiopia1');
 /////////////////////////////////////////////////////////////////////////////
 
 // Make the training dataset.....
 // Make the training dataset.
var training_points = WaterLand.merge(CropLand).merge(BarrenLand).merge(RockyLand).merge(Forest).merge(UrbanLand);
var training_data = L8.sampleRegions({collection: training_points, properties:['LC'], scale: 30})
var classifier = ee.Classifier.smileCart()
var classifier = classifier.train({features:training_data, classProperty: 'LC', inputProperties: ["B1", "B2", "B3","B4", "B5", "B6","B7", "B10", "B11"]});
var classified_image = L8.classify(classifier);
Map.addLayer(classified_image, {min:0, max:5, palette:['#0305d6', '#98ff00', '#78551c', '#ffc82d', '#00ffff', '#bf04c2']}, 'classified image1');
 
 //Exporting the result to Assets Folder
Export.image.toAsset({
    image: classified_image,
    description:  'spclassifiedethio1',
    assetId: 'users/ermdejab/spclassarea1ethio',
    scale: 30,
    region: eth,
    maxPixels: 1e13,
    crs: 'EPSG:32749' 
  });
 //Exporting the result to Drive
Export.image.toDrive({
image: classified_image,
folder: "GEE",
scale: 30,
maxPixels: 1e13,
});
  
  
  
 // Calculating Area for each category
var area = ee.Image.pixelArea().divide(10e6).addBands(classified_image).reduceRegion({
  reducer:ee.Reducer.sum().group({
    groupField: 1, 
    groupName: 'Study_area'}),
    geometry: eth,
    scale: 1000,
    //crs, crsTransform, bestEffort, maxPixels, tileScale)
    maxPixels: 1e13 })
    
    print(area)
    
// ******************************************************************************   
   // FURTHER ENHANCEMENT - delete below if unnecessary 
    //Merge into one FeatureCollection
var valNames = WaterLand.merge(CropLand).merge(BarrenLand).merge(RockyLand).merge(Forest).merge(UrbanLand);
var validation = classified_image.sampleRegions({
  collection: valNames,
  properties: ['LC'],
  scale: 30,
});
print(validation);
//Compare the landcover of your validation data against the classification result
var testAccuracy = validation.errorMatrix('LC', 'classification');
//Print the error matrix to the console
print('Validation error matrix: ', testAccuracy);
//Print the overall accuracy to the console
print('Validation overall accuracy: ', testAccuracy.accuracy());
///////////////////////////////////////////////////// Presentation
// set position of panel
var legend = ui.Panel({
  style: {
    position: 'bottom-left',
    padding: '8px 15px'
  }
});
 
// Create legend title
var legendTitle = ui.Label({
  value: 'LandCoverClassification',
  style: {
    fontWeight: 'bold',
    fontSize: '18px',
    margin: '0 0 4px 0',
    padding: '0'
    }
});
 
// Add the title to the panel
legend.add(legendTitle);
 
// Creates and styles 1 row of the legend.
var makeRow = function(color, name) {
 
      // Create the label that is actually the colored box.
      var colorBox = ui.Label({
        style: {
          backgroundColor: '#' + color,
          // Use padding to give the box height and width.
          padding: '8px',
          margin: '0 0 4px 0'
        }
      });
 
      // Create the label filled with the description text.
      var description = ui.Label({
        value: name,
        style: {margin: '0 0 4px 6px'}
      });
 
      // return the panel
      return ui.Panel({
        widgets: [colorBox, description],
        layout: ui.Panel.Layout.Flow('horizontal')
      });
};
 
//  Palette with the colors
var palette =['0305d6', '98ff00', '78551c','ffc82d', '00ffff', 'bf04c2'];
 
// name of the legend
var names = ['Water body','Crop land','Bare Land', 'Rocky Land', 'Forest', 'Urban Areas'];
 
// Add color and and names
for (var i = 0; i < 6; i++) {
  legend.add(makeRow(palette[i], names[i]));
  }  
 
// add legend to map (alternatively you can also print the legend to the console)
Map.add(legend);
// Create the title label.
var title = ui.Label('Land Cover Landuse Classification Map of Ethiopia - 2021©ErmiasDejene');
title.style().set('position', 'top-center');
Map.add(title);
// Create the title label.
var title = ui.Label('Prepared by: Ermias Dejene Abesha©2021');
title.style().set('position', 'bottom-right');
Map.add(title);
