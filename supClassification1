// Soil Moisture Data of Ethiopia
 // Import soil smap data
var smap = ee.ImageCollection('NASA_USDA/HSL/SMAP10KM_soil_moisture')
  .filterDate('2019-03-09', '2021-09-09')
  .select("ssm")
  .filterBounds(eth);
  
// Display the map
var smap2021 = ee.ImageCollection(smap)
  .filterDate('2021-03-09', '2021-12-31')
  .select("ssm")
  .median();
  
var smapVis = {
  min:0.0,
  max:28.0,
  palette: ['0300ff', '418504', 'efff07', 'ff0303'],
}
Map.addLayer(smap2021.clip(eth), smapVis, "SMAP2021")
